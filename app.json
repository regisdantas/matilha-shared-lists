{
  "expo": {
    "name": "matilha-shared-lists",
    "slug": "matilha-shared-lists",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/matilha.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "jsEngine": "jsc", 
      "supportsTablet": true
    },
    "android": {
      "jsEngine": "jsc", 
      "adaptiveIcon": {
        "foregroundImage": "./assets/matilha.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "static",
      "build": {
        "assetPrefix": "./"
      }
    },
    "plugins": [
      "expo-router",
      "expo-web-browser",
      "expo-secure-store",
      "expo-font"
    ]
  }
}
